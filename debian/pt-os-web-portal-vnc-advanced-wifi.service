[Unit]
Description=pi-topOS advanced wi-fi setup over VNC
Documentation=https://knowledgebase.pi-top.com/knowledge

[Service]
Type=simple
RemainAfterExit=yes
Restart=no
ExecStart=/usr/bin/pt-os-web-portal-vnc-advanced-wifi start
ExecStop=/usr/bin/pt-os-web-portal-vnc-advanced-wifi stop

[Install]
WantedBy=multi-user.target
